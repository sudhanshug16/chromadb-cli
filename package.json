{
  "name": "chromadb-cli",
  "version": "0.1.2",
  "description": "CLI for chromadb",
  "author": "Sudhanshu Gautam @sudhanshug16",
  "bin": {
    "chromadb": "./bin/run"
  },
  "homepage": "https://github.com/sudhanshug16/chromadb-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "sudhanshug16/chromadb-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2.15.0",
    "@oclif/plugin-help": "^5.2.19",
    "@oclif/plugin-update": "^3.2.3",
    "chromadb": "^1.5.11",
    "keytar": "^7.9.0",
    "openai": "^4.10.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.5.6",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.55",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-config-prettier": "^9.0.0",
    "mocha": "^9",
    "prettier": "^3.0.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "chromadb",
    "dirname": "chromadb",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-update"
    ],
    "topicSeparator": " ",
    "topics": {
      "collection": {
        "description": "Access collections in chromadb"
      },
      "items": {
        "description": "Access items in a collection in chromadb"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/sudhanshug16/chromadb-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
